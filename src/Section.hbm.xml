<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<!-- topic的配置文件 -->
	<class name="com.forum.model.Section" table="section">

		<id name="sectionID" column="sectionID" type="int">
			<generator class="increment"></generator>
		</id>

		<property name="parentSectionID" column="parentSectionID" type="java.lang.Integer"></property>
		<property name="sectionName"></property>
		<property name="sectionIntro"></property>
		
	</class>
</hibernate-mapping>
